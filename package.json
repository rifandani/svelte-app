{
  "name": "svelte-template",
  "private": true,
  "version": "0.0.0",
  "description": "Svelte template with typescript, vite, vitest, testing-library, router, msw, tailwindcss, tanstack svelte query, zod",
  "license": "MIT",
  "author": "Tri Rizeki Rifandani",
  "packageManager": "pnpm@7.28.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write --plugin-search-dir=. ./**/*.{html,svelte,ts,js}",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "dev": "vite",
    "start": "npm-run-all --parallel dev typesafe-i18n",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "typesafe-i18n": "typesafe-i18n",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitest/coverage-c8": "^0.31.3",
    "@vitest/ui": "^0.31.3",
    "autoprefixer": "^10.4.14",
    "daisyui": "^2.52.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "jsdom": "^22.1.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.3"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.2.0",
    "@rifandani/nxact-yutiriti": "^1.2.0",
    "@tanstack/svelte-query": "^4.29.11",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "svelte-spa-router": "^3.3.0",
    "tailwind-merge": "^1.12.0",
    "theme-change": "^2.5.0",
    "type-fest": "^3.11.1",
    "typesafe-i18n": "^5.24.3",
    "zod": "^3.21.4"
  }
}
