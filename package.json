{
  "name": "svelte-app",
  "private": true,
  "version": "0.0.0",
  "description": "Bulletproof Svelte 4 SPA Template",
  "license": "MIT",
  "author": "Tri Rizeki Rifandani",
  "type": "module",
  "packageManager": "pnpm@8.7.4",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write --plugin-search-dir=. ./**/*.{html,svelte,ts,js}",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "msw:init": "msw init public/ --save",
    "msw:delete": "rm -rf public/mockServiceWorker.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "typesafe-i18n": "typesafe-i18n",
    "dev": "vite",
    "start": "npm-run-all --parallel dev typesafe-i18n",
    "build": "pnpm msw:delete && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vite preview --outDir html",
    "test:preview:coverage": "vite preview --outDir coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/svelte": "^3.1.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/eslint-plugin-query": "^4.34.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/svelte": "^4.0.3",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/coverage-istanbul": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.7.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-svelte": "^2.33.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "msw": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "@felte/validator-zod": "^1.0.17",
    "@rifandani/nxact-yutiriti": "^1.2.2",
    "@tanstack/svelte-query": "^4.35.3",
    "axios": "^1.5.0",
    "felte": "^1.2.12",
    "svelte-spa-router": "^3.3.0",
    "tailwind-merge": "^1.14.0",
    "type-fest": "^4.3.1",
    "typesafe-i18n": "^5.26.2",
    "zod": "^3.22.2"
  }
}
