{
  "name": "svelte-app",
  "private": true,
  "version": "0.0.0",
  "description": "Bulletproof Svelte 4 SPA Template",
  "license": "MIT",
  "author": "Tri Rizeki Rifandani",
  "type": "module",
  "packageManager": "pnpm@8.14.0",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write --plugin-search-dir=. ./**/*.{html,svelte,ts,js}",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "msw:init": "msw init public/ --save",
    "msw:delete": "rm -rf public/mockServiceWorker.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "typesafe-i18n": "typesafe-i18n",
    "dev": "vite",
    "start": "npm-run-all --parallel dev typesafe-i18n",
    "build": "pnpm msw:delete && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vite preview --outDir html",
    "test:preview:coverage": "vite preview --outDir coverage"
  },
  "devDependencies": {
    "@commitlint/cli": " ^18.4.4",
    "@commitlint/config-conventional": " ^18.4.4",
    "@iconify/svelte": " ^3.1.6",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tailwindcss/forms": " ^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/eslint-plugin-query": " ^5.17.7",
    "@testing-library/jest-dom": " ^6.2.0",
    "@testing-library/svelte": " ^4.0.5",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": " ^29.5.11",
    "@types/jsdom": " ^21.1.6",
    "@types/node": " ^20.10.6",
    "@typescript-eslint/eslint-plugin": " ^6.18.0",
    "@typescript-eslint/parser": " ^6.18.0",
    "@vitest/coverage-istanbul": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "autoprefixer": " ^10.4.16",
    "daisyui": " ^4.5.0",
    "eslint": " ^8.56.0",
    "eslint-config-prettier": " ^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-svelte": " ^2.35.1",
    "eslint-plugin-tailwindcss": " ^3.13.1",
    "eslint-plugin-testing-library": " ^6.2.0",
    "husky": "^8.0.3",
    "jsdom": " ^23.2.0",
    "msw": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.14.0",
    "postcss": " ^8.4.33",
    "prettier": " ^3.1.1",
    "prettier-plugin-svelte": " ^3.1.2",
    "svelte": " ^4.2.8",
    "svelte-check": " ^3.6.2",
    "tailwindcss": " ^3.4.1",
    "tslib": "^2.6.2",
    "typescript": " ^5.3.3",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": " ^4.2.3",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "@felte/validator-zod": "^1.0.17",
    "@melt-ui/svelte": "^0.49.1",
    "@rifandani/nxact-yutiriti": "^1.2.2",
    "@tanstack/svelte-query": " ^5.17.8",
    "axios": " ^1.6.5",
    "bits-ui": " ^0.13.3",
    "felte": "^1.2.12",
    "svelte-spa-router": " ^4.0.0",
    "tailwind-merge": " ^2.2.0",
    "tailwind-variants": " ^0.1.20",
    "type-fest": " ^4.9.0",
    "typesafe-i18n": "^5.26.2",
    "zod": " ^3.22.4"
  }
}