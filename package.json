{
  "name": "svelte-app",
  "private": true,
  "version": "0.0.0",
  "description": "Bulletproof Svelte 4 SPA Template",
  "license": "MIT",
  "author": "Tri Rizeki Rifandani",
  "type": "module",
  "packageManager": "pnpm@8.6.9",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write --plugin-search-dir=. ./**/*.{html,svelte,ts,js}",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "msw:init": "msw init public/ --save",
    "msw:delete": "rm -rf public/mockServiceWorker.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "typesafe-i18n": "typesafe-i18n",
    "dev": "vite",
    "start": "npm-run-all --parallel dev typesafe-i18n",
    "build": "pnpm msw:delete && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vite preview --outDir html",
    "test:preview:coverage": "vite preview --outDir coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify/svelte": "^3.1.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/svelte": "^4.0.3",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^20.4.5",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-istanbul": "^0.32.4",
    "@vitest/ui": "^0.32.4",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.5.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.32.4"
  },
  "dependencies": {
    "@felte/validator-zod": "^1.0.16",
    "@rifandani/nxact-yutiriti": "^1.2.2",
    "@tanstack/svelte-query": "^4.32.0",
    "felte": "^1.2.11",
    "redaxios": "^0.5.1",
    "svelte-spa-router": "^3.3.0",
    "tailwind-merge": "^1.14.0",
    "type-fest": "^3.13.1",
    "typesafe-i18n": "^5.25.1",
    "zod": "^3.21.4"
  }
}
