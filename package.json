{
  "name": "svelte-template",
  "private": true,
  "version": "0.0.0",
  "description": "Svelte template with typescript, vite, vitest, testing-library, router, msw, tailwindcss, tanstack svelte query, zod",
  "license": "MIT",
  "author": "Tri Rizeki Rifandani",
  "type": "module",
  "packageManager": "pnpm@7.28.0",
  "msw": {
    "workerDirectory": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write --plugin-search-dir=. ./**/*.{html,svelte,ts,js}",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "lint:fix": "eslint . --ext .js,.ts,.svelte --fix",
    "msw:init": "msw init public/ --save",
    "msw:delete": "rm -rf public/mockServiceWorker.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "typesafe-i18n": "typesafe-i18n",
    "dev": "vite",
    "start": "npm-run-all --parallel dev typesafe-i18n",
    "build": "pnpm msw:delete && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vite preview --outDir html",
    "test:preview:coverage": "vite preview --outDir coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@tsconfig/svelte": "^4.0.1",
    "@types/node": "^20.3.1",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/coverage-istanbul": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.5",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.2",
    "svelte-check": "^3.4.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@felte/validator-zod": "^1.0.16",
    "@rifandani/nxact-yutiriti": "^1.2.0",
    "@tanstack/svelte-query": "^4.29.15",
    "axios": "^1.4.0",
    "felte": "^1.2.10",
    "svelte-spa-router": "^3.3.0",
    "tailwind-merge": "^1.13.2",
    "theme-change": "^2.5.0",
    "type-fest": "^3.12.0",
    "typesafe-i18n": "^5.24.3",
    "zod": "^3.21.4"
  }
}
